{
  "name": "@idea2app/taro-vant-mobx-ts",
  "version": "1.0.0",
  "private": true,
  "description": "Taro project scaffold based on TypeScript, Preact, MobX & Vant",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "lint-staged",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@taroify/core": "0.1.0-alpha.1",
    "@taroify/icons": "0.1.0-alpha.1",
    "@tarojs/components": "3.5.2",
    "@tarojs/react": "3.5.2",
    "@tarojs/runtime": "3.5.2",
    "@tarojs/taro": "3.5.2",
    "@vant/area-data": "^1.3.1",
    "bootstrap": "^5.2.0",
    "koajax": "^0.8.1",
    "mobx": "^4.15.7",
    "mobx-react": "^6.3.1",
    "mobx-restful": "^0.4.0-rc.2",
    "preact": "^10.10.2",
    "query-string": "^7.1.1",
    "web-utility": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@prefresh/babel-plugin": "^0.4.3",
    "@prefresh/core": "^1.3.4",
    "@prefresh/webpack": "~3.3.4",
    "@tarojs/cli": "3.5.2",
    "@tarojs/helper": "3.5.2",
    "@tarojs/plugin-framework-react": "3.5.2",
    "@tarojs/plugin-html": "3.5.2",
    "@tarojs/plugin-platform-alipay": "3.5.2",
    "@tarojs/plugin-platform-jd": "3.5.2",
    "@tarojs/plugin-platform-qq": "3.5.2",
    "@tarojs/plugin-platform-swan": "3.5.2",
    "@tarojs/plugin-platform-tt": "3.5.2",
    "@tarojs/plugin-platform-weapp": "3.5.2",
    "@tarojs/router": "3.5.2",
    "@tarojs/shared": "3.5.2",
    "@tarojs/taro-h5": "3.5.2",
    "@tarojs/webpack5-runner": "3.5.2",
    "@types/react": "^17.0.48",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.5.2",
    "eslint": "^8.21.0",
    "eslint-config-taro": "3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "stylelint": "^14.10.0",
    "typescript": "~4.7.4",
    "webpack": "^5.74.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.{md,js,ts,tsx,json,yml}": "prettier --write"
  }
}
